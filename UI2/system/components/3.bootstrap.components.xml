<?xml version="1.0" encoding="UTF-8"?>
<reg-info>
	<!-- 组件目录注册  -->
	<component-dirs>
		bootstrap/panel, 
		bootstrap/carousel,
		bootstrap/accordion, 
		bootstrap/row,
		bootstrap/table,
		bootstrap/tabs,bootstrap/dropdown,
		bootstrap/alert, 
		bootstrap/navs,
		bootstrap/navbar,
		bootstrap/dialog,
		bootstrap/inputGroup,
		bootstrap/label,
		bootstrap/scrollSpy,
		bootstrap/badge,
		bootstrap/listGroup,
		bootstrap/pagination,
		bootstrap/pager,
		bootstrap/controlLabel,
		bootstrap/form
	</component-dirs>
	
	<!-- 组件工具箱配置 -->
	<toolbox>
	
		<catalog name="布局">
			<item component-name="$UI/system/components/bootstrap/accordion/accordion(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/tabs/tabs(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/row/row(bootstrap)"/>
		</catalog>
			
		<catalog name="表单">
 			<item component-name="$UI/system/components/bootstrap/form/form(bootstrap)" />
			<item component-name="$UI/system/components/bootstrap/form/form(bootstrap)#formGroup" />
			<item component-name="$UI/system/components/bootstrap/inputGroup/inputGroup(bootstrap)"/>
		</catalog>
		

		<catalog name="扩展">
			<item component-name="$UI/system/components/bootstrap/table/table(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/dropdown/dropdown(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/navs/navs(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/navbar/navbar(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/badge/badge(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/pagination/pagination(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/pager/pager(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/scrollSpy/scrollSpy(bootstrap)" device="pc"/>
		</catalog>

		<catalog name="对话框">
			<item component-name="$UI/system/components/bootstrap/alert/alert(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/dialog/dialog(bootstrap)"/>
		</catalog>

		<catalog name="bootstrap" order="10">
			<item component-name="$UI/system/components/bootstrap/panel/panel(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/accordion/accordion(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/table/table(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/tabs/tabs(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/dropdown/dropdown(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/navs/navs(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/navbar/navbar(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/carousel/carousel(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/alert/alert(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/row/row(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/dialog/dialog(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/inputGroup/inputGroup(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/controlLabel/controlLabel(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/label/label(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/badge/badge(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/scrollSpy/scrollSpy(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/listGroup/listGroup(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/pagination/pagination(bootstrap)"/>
			<item component-name="$UI/system/components/bootstrap/pager/pager(bootstrap)"/>
 			<item component-name="$UI/system/components/bootstrap/form/form(bootstrap)" />
			<item component-name="$UI/system/components/bootstrap/form/form(bootstrap)#formGroup(bootstrap)" />
		</catalog>
	</toolbox>
	
	<depend-css>
         <item>$UI/system/components/bootstrap.min.css</item>
 	</depend-css>
</reg-info>